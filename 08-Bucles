const PROTOCOL = "";
switch(PROTOCOL){
  case "HTTP":
    console.log("El sitio es inseguro");
    break;
    case "HTTPS":
      console.log("El sitio es seguro");
      break;
    case "SSH":
      console.log("Super admin en servidor");
      break;
    default:
      console.log("No encontrado");
      break;
}




